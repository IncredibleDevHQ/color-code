  "_moduleAliases": {
    "@root"      : "."
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "@types/tape": "^4.13.0",
    "durations": "^3.4.1",
    "onigasm": "^2.2.3",
    "oniguruma": "^7.2.0",
    "tape": "^4.13.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "vscode-oniguruma": "^1.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jwks-rsa": "^1.9.0",
    "kafkajs": "^1.12.0",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2"
  }
